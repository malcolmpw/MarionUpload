<Window x:Class="MarionUpload.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="clr-namespace:MarionUpload.Views"
        mc:Ignorable="d"
        Title="MainWindow" >
    <Grid>
        <StackPanel >
            <TextBlock Text="Chose Task to Upload or Import Data" FontSize="20" Margin="10"/>
            <TabControl Name="tabImportAndUploadTasks" Margin="10">
                <TabItem Header="Owners">
                    <view:vwOwner/>
                </TabItem>
                <TabItem Header="Property">
                    <view:vwProperty />
                </TabItem>
                <TabItem Header="Account">
                    <view:vwAccount/>
                </TabItem>
                <TabItem Header="tlkpUnit">
                    <view:vwUnit/>
                </TabItem>
                <!--<TabItem Header="CadUnit">
                    <view:vwCadUnit/>
                </TabItem>-->
                <TabItem Header="UnitProperty">
                    <view:vwUnitProperty/>
                </TabItem>
                <TabItem Header="Agents">
                    <view:vwAgent/>
                </TabItem>
            </TabControl>
        </StackPanel>
    </Grid>
</Window>
